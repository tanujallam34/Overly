# Environment variables example
# Copy to .env and fill real values

# Postgres connection
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/overly-cricket-database?schema=public"

# API
PORT=3000

# JWT secrets (generate secure random strings in production)
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"
JWT_REFRESH_SECRET="your-super-secret-refresh-key-change-this-in-production"

# JWT expiration times (in seconds)
JWT_EXPIRES_IN=3600
JWT_REFRESH_EXPIRES_IN=604800
